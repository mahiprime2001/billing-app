{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Siri-Admin-app",
  "version": "0.1.70",
  "identifier": "com.siriadmin.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Siri-Admin-app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default-capabilities",
        "main-capability"
      ]
    },
    "withGlobalTauri": true
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/mahiprime2001/billing-app/releases/latest/download/update.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZENzM1QzQwMjUxRkYxNkMKUldSczhSOGxRRnh6L1ZlcUQzR2J4UTF0VEpSRVBFQXBDalF0TDZ1R2ZEVTZWSzVDSGRaaTh3MjYK",
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "bundle": {
    "copyright": "all rights are reserved by IFLEON",
    "publisher": "IFLEON",
    "android": {
      "minSdkVersion": 24
    },
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "externalBin": [
      "binaries/Siriadmin-backend"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "en-US"
      },
      "nsis": {
        "installerIcon": "icons/icon.ico",
        "installMode": "perMachine",
        "displayLanguageSelector": false
      }
    }
  }
}
