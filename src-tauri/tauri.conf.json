{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Siri-Admin-app",
  "version": "0.1.25",
  "identifier": "com.siriadmin.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Siri-Admin-app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default-capabilities"
      ]
    },
    "withGlobalTauri": true
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/mahiprime2001/billing-app/releases/download/latest/updater.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZENzM1QzQwMjUxRkYxNkMKUldSczhSOGxRRnh6L1ZlcUQzR2J4UTF0VEpSRVBFQXBDalF0TDZ1R2ZEVTZWSzVDSGRaaTh3MjYK"
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png",
      "icons/icon@2x.png",
      "icons/icon@3x.png",
      "icons/icon@4x.png",
      "icons/icon@5x.png",
      "icons/icon@6x.png",
      "icons/icon@7x.png",
      "icons/icon@8x.png",
      "icons/icon@9x.png",
      "icons/icon@10x.png",
      "icons/icon@11x.png",
      "icons/icon@12x.png",
      "icons/icon@13x.png",
      "icons/icon@14x.png",
      "icons/icon@15x.png",
      "icons/icon@16x.png",
      "icons/icon@2x.ico"
    ],
    "externalBin": [
      "binaries/Siriadmin-backend"
    ]
  }
}
