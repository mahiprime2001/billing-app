{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Siri_Art_Jewellery",
  "version": "0.1.0",
  "identifier": "com.Siri.dev",
  "build": {
    "frontendDist": "../.next",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Siri Art Jewellery",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://updates.ifleon.com/updater.json"
      ],
      "dialog": true,
