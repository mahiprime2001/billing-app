{
  "identifier": "main-capability",
  "description": "Main capability for Siri Admin App",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:window:default",
    "core:webview:default",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn", 
    "shell:allow-kill",
    "log:default",
    "log:allow-log",
    "dialog:default",
    "updater:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "Siriadmin-backend",
          "sidecar": true
        }
      ]
    }
  ]
}
